{
    "name": "backendtea/architect",
    "description": "Architectural checks for your codebase",
    "type": "library",
    "keywords": [
        "dev"
    ],
    "require-dev": {
        "phpunit/phpunit": "^10.0||^11.0",
        "phpstan/phpstan": "^1.11",
        "doctrine/coding-standard": "^12.0",
        "phpstan/phpstan-phpunit": "^1.4",
        "phpstan/extension-installer": "^1.4"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "BackEndTea\\Architect\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BackEndTea\\Architect\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Gert de Pagter",
            "email": "backendtea@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/console": "^6.4||^7.1",
        "symfony/finder": "^6.4||^7.1",
        "roave/better-reflection": "^6.25",
        "nikic/php-parser": "^4.18|^5.1",
        "symfony/filesystem": "^6.4||^7.1",
        "psr/container": "^2.0"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true
        }
    },
    "bin": [
        "bin/architect"
    ]
}
